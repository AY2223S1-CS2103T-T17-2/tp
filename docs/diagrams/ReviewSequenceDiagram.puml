@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":NutriGoalsParser" as NutriGoalsParser LOGIC_COLOR
participant ":ReviewCommand" as ReviewCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("review")
activate LogicManager

LogicManager -> NutriGoalsParser : parseCommand("review")
activate NutriGoalsParser

create ReviewCommand
NutriGoalsParser -> ReviewCommand
activate ReviewCommand

ReviewCommand --> NutriGoalsParser
deactivate ReviewCommand

NutriGoalsParser --> LogicManager
deactivate NutriGoalsParser

LogicManager -> ReviewCommand : execute()
activate ReviewCommand

ReviewCommand -> Model : getTotalCalorie()
activate Model

Model --> ReviewCommand
deactivate Model

ReviewCommand -> Model : getCalorieDifference()
activate Model

Model --> ReviewCommand
deactivate Model

ReviewCommand -> Model : getCalorieTarget()
activate Model

Model --> ReviewCommand
deactivate Model

create CommandResult
ReviewCommand -> CommandResult
activate CommandResult

CommandResult --> ReviewCommand
deactivate CommandResult

ReviewCommand --> LogicManager
deactivate ReviewCommand

[<--LogicManager
deactivate LogicManager

@enduml
